                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:drawableLeft="@mipmap/ask_order_front_ed_time"
                        android:drawablePadding="10dp"
                        android:gravity="bottom"
                        android:text="结束时间" />

                    <TextView
                        android:id="@+id/driver_charter_setting_ed_time_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom"
                        android:layout_marginLeft="10dp"
                        android:text="" />
                </LinearLayout>

                <ImageView
                    android:id="@+id/driver_charter_setting_ed_time_add_iv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:src="@mipmap/ask_order_go_iv" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/driver_charter_setting_day_off_rl"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:id="@+id/driver_charter_setting_day_off_ll"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="15dp"
                        android:drawableLeft="@mipmap/ask_order_front_costomer_num"
                        android:drawablePadding="10dp"
                        android:gravity="bottom"
                        android:text="假日上浮比例" />
                </LinearLayout>

                <EditText
                    android:id="@+id/driver_charter_setting_day_off_et"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_toLeftOf="@+id/driver_charter_setting_day_off_tv"
                    android:background="@null"
                    android:gravity="bottom|right"
                    android:hint="请填入假日上浮比"
                    android:textColorHint="#D2D2D2"
                    android:textSize="15sp" />

                <TextView
                    android:id="@+id/driver_charter_setting_day_off_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentRight="true"
                    android:layout_gravity="bottom"
                    android:layout_marginLeft="10dp"
                    android:text=" %" />
            </RelativeLayout>


            <Button
                android:id="@+id/driver_charter_setting_sub_btn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="30dp"
                android:background="@drawable/btn_orange_arc_bg"
                android:text="下一步"
                android:textColor="#FFFFFF" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:text="说明:计算器价格仅供参考,不作为实际租车价格!"
                android:textColor="#D2D2D2" />
        </LinearLayout>
    </ScrollView>
</LinearLayout>