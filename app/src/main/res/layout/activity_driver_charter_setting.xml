            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@drawable/btn_select_white_to_gray_bg"
            android:button="@null"
            android:gravity="center"
            android:padding="5dp"
            android:text="特殊景区"
            android:textSize="17sp" />
