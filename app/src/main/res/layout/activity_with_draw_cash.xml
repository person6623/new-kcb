<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/with_draw_cash_title_rl"
        android:layout_width="match_parent"
        android:layout_height="43dp">

        <ImageView
            android:id="@+id/with_draw_cash_back_iv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="7dp"
            android:background="@mipmap/customer_details_back" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="提现记录"
            android:textColor="#F89928"
            android:textSize="17sp" />
    </RelativeLayout>

    <com.kcb360.newkcb.xlistviewaliter.XListView
        android:id="@+id/with_draw_cash_xlv"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    </com.kcb360.newkcb.xlistviewaliter.XListView>
</LinearLayout>